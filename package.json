{
  "name": "dungeon-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Dungeon building game in fantasy world - 2D TypeScript game",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tsc": "tsc --version && tsc -b",
    "prettier-check": "prettier -v && prettier --check ./src",
    "prettier-fix": "prettier -v && prettier --check ./src --write",
    "detect-cycles": "madge --extensions ts,tsx,js,jsx --ts-config tsconfig.json --circular ./src/index.tsx",
    "detect-unused": "madge --extensions ts,tsx,js,jsx --ts-config tsconfig.json --orphans ./src",
    "qwe": "qwe"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "idb": "^8.0.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.6",
    "neverthrow": "^8.2.0",
    "phaser": "^3.90.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.17",
    "ts-expect": "^1.3.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vueuse/core": "^14.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rolldown-vite": "^7.2.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "@latest"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
