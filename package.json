{
  "name": "dungeon-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Dungeon building game in fantasy world - 2D TypeScript game",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tsc": "tsc --version && tsc -b",
    "prettier-check": "prettier -v && prettier --check ./src",
    "prettier-fix": "prettier -v && prettier --check ./src --write",
    "detect-cycles": "madge --extensions ts,tsx,js,jsx --ts-config tsconfig.json --circular ./src/index.tsx",
    "detect-unused": "madge --extensions ts,tsx,js,jsx --ts-config tsconfig.json --orphans ./src",
    "qwe": "qwe"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.6",
    "neverthrow": "^8.2.0",
    "phaser": "^3.90.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "ts-expect": "^1.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@tailwindcss/vite": "^4.1.17",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "rolldown-vite": "^7.2.2",
    "vite": "@latest"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
